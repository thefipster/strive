<MudAppBar Elevation="1">
    <MudIcon Class="ml-3" Icon="@Icons.Material.Outlined.GpsFixed" Color="Color.Inherit"/>
    <MudLink Style="text-decoration: none;" Color="Color.Inherit" Href="https://localhost:7073">
        <MudText Typo="Typo.h5" Class="ml-7">Strive</MudText>
    </MudLink>
    <MudLink Style="text-decoration: none;" Color="Color.Inherit" Href="/">
        <MudText Typo="Typo.h6" Class="ml-4">Harvester</MudText>
    </MudLink>
    <MudSpacer/>
    <MudStack Row="true" Justify="Justify.Center" Spacing="3" AlignItems="AlignItems.Center">
        <MudIcon Icon="@Icons.Material.Rounded.Login"/>
        <MudText>@_queueIn / s</MudText>
        <MudText>|</MudText>
        <MudIcon Icon="@Icons.Material.Rounded.Pending"/>
        <MudText>@_queueCount</MudText>
        <MudText>|</MudText>
        <MudText>@_queueOut / s</MudText>
        <MudIcon Icon="@Icons.Material.Rounded.Logout"/>
    </MudStack>
    <MudSpacer/>
    <NavMenu/>
</MudAppBar>